{
  "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "aspName": {
      "type": "String"
    },
    "aspLocation": {
      "type": "String"
    },
    "aspSku": {
      "type": "String"
    },
    "aspSkuCode": {
      "type": "String"
    },
    "workerSize": {
      "type": "String"
    },
    "workerSizeId": {
      "type": "String"
    },
    "aspNumberOfWorkers": {
      "type": "String"
    },
    "aspHostingEnvironment": {
      "type": "String"
    }
  },
  "resources": [
    {
      "type": "Microsoft.Web/serverfarms",
      "sku": {
        "Tier": "[parameters('aspSku')]",
        "Name": "[parameters('aspSkuCode')]"
      },
      "kind": "",
      "name": "[parameters('aspName')]",
      "apiVersion": "2016-03-01",
      "location": "[parameters('aspLocation')]",
      "properties": {
        "name": "[parameters('aspName')]",
        "workerSize": "[parameters('workerSize')]",
        "workerSizeId": "[parameters('workerSizeId')]",
        "numberOfWorkers": "[parameters('aspNumberOfWorkers')]",
        "reserved": false,
        "hostingEnvironment": "[parameters('aspHostingEnvironment')]"
      }
    }
  ]
}